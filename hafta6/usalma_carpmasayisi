#include<stdio.h>
#include<conio.h>


int f(int a,int b)
{
    int i;
    int x=1;
    int step=0;
while(b)
{
    if(b%2==0){
            a=a*a;
            b=b/2;
        }else{
            x=x*a;
            b--;
        }
        step++;
}

printf("\n step= %d ",step);
return x;

}
main()
{
    int a,b;
    printf("\n a = ");
    scanf("%d",&a);
    printf("\n b = ");
    scanf("%d",&b);



    printf("\n a ussu b = %d ",f(a,b));


getch();
return 0;

}
